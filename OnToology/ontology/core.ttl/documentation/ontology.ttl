@prefix : <http://iot.linkeddata.es/def/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://iot.linkeddata.es/def/core#> .

<http://iot.linkeddata.es/def/core#> rdf:type owl:Ontology ;
                                      owl:imports <http://iot.linkeddata.es/def/wot#> ,
                                                  <http://www.w3.org/ns/org#> ,
                                                  <http://www.w3.org/ns/sosa/> ;
                                      <http://purl.org/dc/elements/1.1/publisher> <http://www.oeg-upm.net/> ;
                                      <http://purl.org/dc/elements/1.1/title> "Vicinity core model"@en ;
                                      <http://purl.org/dc/terms/contributor> <http://www.oeg-upm.net/index.php/en/universitystaff/402-fserena/> ;
                                      <http://purl.org/dc/terms/creator> <http://purl.org/net/mpoveda> ,
                                                                         <http://www.garcia-castro.com/foaf.rdf#me> ;
                                      <http://purl.org/dc/terms/license> <http://purl.org/NET/rdflicense/cc-by4.0> ;
                                      <http://purl.org/vocab/vann/preferredNamespacePrefix> "vcnt-core" ;
                                      <http://purl.org/vocab/vann/preferredNamespaceUri> "http://iot.linkeddata.es/def/core" ;
                                      rdfs:comment "This ontology represents the core terms to allow interoperability in an IoT context based on  VICINITY technological solutions."@en ;
                                      owl:versionInfo "0.2.4" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/source
<http://purl.org/dc/elements/1.1/source> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/contributor
<http://purl.org/dc/terms/contributor> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#cardinality
owl:cardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/06/sw-vocab-status/ns#term_status
<http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#hadPrimarySource
<http://www.w3.org/ns/prov#hadPrimarySource> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://iot.linkeddata.es/def/core#aboutFeatureOfInterest
:aboutFeatureOfInterest rdf:type owl:ObjectProperty ,
                                 owl:FunctionalProperty ;
                        rdfs:comment "Auxiliary property created for the n-ary class core:FeatureProperty to link instances of such class to the feature of interest it is about."@en ;
                        rdfs:label "about feature of interest"@en .


###  http://iot.linkeddata.es/def/core#aboutProperty
:aboutProperty rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:comment "Auxiliary property created for the n-ary class core:FeatureProperty to link instances of such class to the (ssn) proeprty it is about."@en ;
               rdfs:label "about property"@en .


###  http://iot.linkeddata.es/def/core#actsOnFeatureProperty
:actsOnFeatureProperty rdf:type owl:ObjectProperty ;
                       rdfs:range :FeatureProperty ;
                       rdfs:comment "Auxiliary property created for the n-ary class core:FeatureProperty. This property links devices (sensor or actuators) or interaction patterns to instances of the n-ary class which will be the ones linking to the properties and features of interest which the device or interaction pattern acts on."@en ;
                       rdfs:label "acts on feature property"@en .


###  http://iot.linkeddata.es/def/core#avatar
:avatar rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/depiction> ;
        rdfs:comment "A link from an entity to the image identifying it in a social network or system."@en ;
        rdfs:label "avatar"@en .


###  http://iot.linkeddata.es/def/core#describes
:describes rdf:type owl:ObjectProperty ;
           owl:inverseOf :isDescribedBy ;
           rdfs:comment "Link from a thing description to the thing being described"@en ;
           rdfs:label "describes thing"@en .


###  http://iot.linkeddata.es/def/core#describesEcosystem
:describesEcosystem rdf:type owl:ObjectProperty ;
                    owl:inverseOf :isEcosystemDescribedBy ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:comment "Link between a thing ecosystem description and the ecosystem being described."@en ;
                    rdfs:label "describes ecosystem"@en ;
                    owl:deprecated "true"^^xsd:boolean .


###  http://iot.linkeddata.es/def/core#exposes
:exposes rdf:type owl:ObjectProperty ;
         rdfs:domain :VirtualThing ;
         rdfs:range <http://iot.linkeddata.es/def/wot#Thing> ;
         rdfs:label "A property representing the Thing (Virtual or Physical) that a service exposes."@en ,
                    "exposes"@en .


###  http://iot.linkeddata.es/def/core#expressedInFormat
:expressedInFormat rdf:type owl:ObjectProperty ;
                   rdfs:domain :Value ;
                   rdfs:range <http://iot.linkeddata.es/def/wot#DataSchema> ;
                   rdfs:comment "Link from a value to the data format in which it is expressed."@en ;
                   rdfs:label "expressed in format"@en .


###  http://iot.linkeddata.es/def/core#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              owl:inverseOf :isComponentOf ;
              rdfs:comment "A link between an ecosystem and the things that compose such ecosystem."@en ;
              rdfs:label "has component"@en .


###  http://iot.linkeddata.es/def/core#hasMaxValue
:hasMaxValue rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf rdf:value ;
             rdfs:range rdfs:Resource ;
             rdfs:label "hasMaxValue"@en .


###  http://iot.linkeddata.es/def/core#hasMinValue
:hasMinValue rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf rdf:value ;
             rdfs:domain rdfs:Resource ;
             rdfs:label "hasMinValue"@en .


###  http://iot.linkeddata.es/def/core#hasNeighbourhood
:hasNeighbourhood rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :isInvolvedIn ;
                  owl:inverseOf :isNeighbourhoodOf ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :Agent ;
                  rdfs:range :Neighbourhood ;
                  rdfs:isDefinedBy "The link between an agent and its neighbourhood. Such neighbourhood will link to all the entities involved in such neighbourhood."@en ;
                  rdfs:label "has neighbourhood"@en .


###  http://iot.linkeddata.es/def/core#hasOwner
:hasOwner rdf:type owl:ObjectProperty ;
          owl:inverseOf :owns ;
          rdfs:isDefinedBy "Link between an entity like a service or a device and its owners or providers."@en ;
          rdfs:label "has owner"@en ,
                     "has provider"@en .


###  http://iot.linkeddata.es/def/core#hasPartnershipWith
:hasPartnershipWith rdf:type owl:ObjectProperty ,
                             owl:SymmetricProperty ;
                    rdfs:domain <http://www.w3.org/ns/org#Organization> ;
                    rdfs:range <http://www.w3.org/ns/org#Organization> ;
                    rdfs:isDefinedBy "A link between two organizations who are in a partnership relation. A partnership is established between organizations, only 2 organizations."@en ;
                    rdfs:label "hasPartnershipWith"@en .


###  http://iot.linkeddata.es/def/core#hasValue
:hasValue rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( <http://iot.linkeddata.es/def/wot#InteractionPattern>
                                      <http://iot.linkeddata.es/def/wot#Thing>
                                    )
                      ] ;
          rdfs:range :Value ;
          rdfs:comment "Indicates a value provided by an interaction pattern or a Thing. In case one ore more values are indicated the order will be defined by the time stamp attributes."@en ;
          rdfs:label "has value"@en .


###  http://iot.linkeddata.es/def/core#hasVendor
:hasVendor rdf:type owl:ObjectProperty ;
           rdfs:isDefinedBy "Link from a device to its vendor or manufacturer or producer."@en ;
           rdfs:label "has producer"@en ,
                      "has vendor"@en .


###  http://iot.linkeddata.es/def/core#interactsUsing
:interactsUsing rdf:type owl:ObjectProperty ;
                rdfs:isDefinedBy "Linkg between an agent and the virtual thing it interatcs with."@en ;
                rdfs:label "interacts using"@en ;
                rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)" .


###  http://iot.linkeddata.es/def/core#involves
:involves rdf:type owl:ObjectProperty ;
          owl:inverseOf :isInvolvedIn ;
          rdfs:isDefinedBy "A link between a neighbourhood and all the agents involved in it."@en ;
          rdfs:label "involves"@en .


###  http://iot.linkeddata.es/def/core#isComponentOf
:isComponentOf rdf:type owl:ObjectProperty ;
               rdfs:comment "A link between a thing and the ecosystems it belongs to."@en ;
               rdfs:label "is component of"@en .


###  http://iot.linkeddata.es/def/core#isComposedBy
:isComposedBy rdf:type owl:ObjectProperty ,
                       owl:TransitiveProperty ;
              rdfs:domain :ComposedThing ;
              rdfs:range owl:Thing ;
              rdfs:comment "A property between a thing and its components. For example, virtual things can contain virtual things while physical things can contain physicial or virtual things."@en ;
              rdfs:label "is composed by"@en .


###  http://iot.linkeddata.es/def/core#isDescribedBy
:isDescribedBy rdf:type owl:ObjectProperty ;
               rdfs:comment "Relation from a thing to its thing descriptions"@en ;
               rdfs:label "is described by"@en .


###  http://iot.linkeddata.es/def/core#isEcosystemDescribedBy
:isEcosystemDescribedBy rdf:type owl:ObjectProperty ,
                                 owl:InverseFunctionalProperty ;
                        rdfs:comment "Link from an ecosystem to the thing ecosystem descriptions that provide information about such ecosystem."@en ;
                        rdfs:label "is ecosystem described by"@en ;
                        owl:deprecated "true"^^xsd:boolean .


###  http://iot.linkeddata.es/def/core#isInvolvedIn
:isInvolvedIn rdf:type owl:ObjectProperty ;
              rdfs:isDefinedBy "A link between an agent and all the neighbouthoods it is involved in."@en ;
              rdfs:label "is involved in"@en .


###  http://iot.linkeddata.es/def/core#isLocatedAt
:isLocatedAt rdf:type owl:ObjectProperty ;
             rdfs:comment "The relation between a phisical thing, for example a device, and the geographical feature in which it is located."@en ;
             rdfs:label "is located at"@en .


###  http://iot.linkeddata.es/def/core#isNeighbourhoodOf
:isNeighbourhoodOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :involves ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :Neighbourhood ;
                   rdfs:range :Agent ;
                   rdfs:isDefinedBy "A link between a neighbourhood and the specific agent around which the neighbourhood is built."@en ;
                   rdfs:label "is neighbourhood of"@en .


###  http://iot.linkeddata.es/def/core#isRelativeTo
:isRelativeTo rdf:type owl:ObjectProperty ;
              rdfs:comment "Link from a relative endpoint to the (non relative) endpoint to which it is relative."@en ;
              rdfs:label "is relative to"@en .


###  http://iot.linkeddata.es/def/core#isRepresentedBy
:isRepresentedBy rdf:type owl:ObjectProperty ;
                 owl:inverseOf :represents ;
                 rdf:type owl:InverseFunctionalProperty ;
                 rdfs:domain owl:Thing ;
                 rdfs:range <http://iot.linkeddata.es/def/wot#Thing> ;
                 rdfs:comment "relation between anything, virtual or physical, and its wot:Thing representation(s)"@en ;
                 rdfs:label "is represented by"@en .


###  http://iot.linkeddata.es/def/core#literalValue
:literalValue rdf:type owl:ObjectProperty ;
              rdfs:domain :Value ;
              rdfs:range rdfs:Resource ;
              rdfs:comment "Literal representation of the observed value."@en ;
              rdfs:label "literal value"@en .


###  http://iot.linkeddata.es/def/core#monitorsFeatureProperty
:monitorsFeatureProperty rdf:type owl:ObjectProperty ;
                         rdfs:range :FeatureProperty ;
                         rdfs:comment "Auxiliary property created for the n-ary class core:FeatureProperty. This property links devices (sensor or actuators) or interaction patterns to instances of the n-ary class which will be the ones linking to the properties and features of interest monitored."@en ;
                         rdfs:label "monitors feature property"@en .


###  http://iot.linkeddata.es/def/core#owns
:owns rdf:type owl:ObjectProperty ;
      rdfs:isDefinedBy "A linkd between an agent and the entities, like devices or services, it provides or owns."@en ;
      rdfs:label "owns"@en ,
                 "provides"@en .


###  http://iot.linkeddata.es/def/core#petitionerItemInvolved
:petitionerItemInvolved rdf:type owl:ObjectProperty ;
                        rdfs:comment "Devices or services belonging to the contract petitioner that are involved in the contract  to use the requested service."@en ;
                        rdfs:label "petitioner item involved"@en .


###  http://iot.linkeddata.es/def/core#represents
:represents rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain <http://iot.linkeddata.es/def/wot#Thing> ;
            rdfs:range owl:Thing ;
            rdfs:comment "relation between a wot:Thing and the virtual or phyisical thing it represents"@en ;
            rdfs:label "represents"@en .


###  http://iot.linkeddata.es/def/core#requestedService
:requestedService rdf:type owl:ObjectProperty ;
                  rdfs:comment "Service requested in the contract"@en ;
                  rdfs:label "requested service"@en .


###  http://iot.linkeddata.es/def/core#serviceOwner
:serviceOwner rdf:type owl:ObjectProperty ;
              rdfs:label "Indicates the organization that owns the service involved in the contract"@en ,
                         "service owner"@en .


###  http://iot.linkeddata.es/def/core#servicePetitioner
:servicePetitioner rdf:type owl:ObjectProperty ;
                   rdfs:comment "Agent who request access to a service by a contract"@en ;
                   rdfs:label "service petitioner"@en .


###  http://www.opengis.net/ont/geosparql#hasGeometry
<http://www.opengis.net/ont/geosparql#hasGeometry> rdf:type owl:ObjectProperty .


###  http://www.opengis.net/ont/geosparql#sfContains
<http://www.opengis.net/ont/geosparql#sfContains> rdf:type owl:ObjectProperty ;
                                                  rdfs:domain <http://www.opengis.net/ont/geosparql#SpatialObject> ;
                                                  rdfs:range <http://www.opengis.net/ont/geosparql#SpatialObject> ;
                                                  <http://purl.org/dc/elements/1.1/creator> "OGC GeoSPARQL 1.0 Standard Working Group" ;
                                                  rdfs:comment "Exists if the subject SpatialObject spatially contains the object SpatialObject. DE-9IM: T*****FF*"@en ;
                                                  rdfs:isDefinedBy <http://www.opengis.net/ont/geosparql> ;
                                                  rdfs:label "contains"@en .


###  http://www.opengis.net/ont/geosparql#sfWithin
<http://www.opengis.net/ont/geosparql#sfWithin> rdf:type owl:ObjectProperty ;
                                                rdfs:domain <http://www.opengis.net/ont/geosparql#SpatialObject> ;
                                                rdfs:range <http://www.opengis.net/ont/geosparql#SpatialObject> ;
                                                <http://purl.org/dc/elements/1.1/creator> "OGC GeoSPARQL 1.0 Standard Working Group" ;
                                                rdfs:comment "Exists if the subject SpatialObject is spatially within the object SpatialObject. DE-9IM: T*F**F***"@en ;
                                                rdfs:isDefinedBy <http://www.opengis.net/ont/geosparql> ;
                                                rdfs:label "within"@en .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#value
rdf:value rdf:type owl:ObjectProperty ;
          rdfs:domain rdfs:Resource ;
          rdfs:range rdfs:Resource ;
          rdfs:comment "Idiomatic property used for structured values." ;
          rdfs:isDefinedBy rdf: ;
          rdfs:label "value" .


###  http://www.w3.org/2003/01/geo/wgs84_pos#location
<http://www.w3.org/2003/01/geo/wgs84_pos#location> rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/sosa/observes
<http://www.w3.org/ns/sosa/observes> owl:propertyChainAxiom ( :monitorsFeatureProperty
                                                              :aboutProperty
                                                            ) .


###  http://www.w3.org/ns/ssn/forProperty
<http://www.w3.org/ns/ssn/forProperty> rdf:type owl:ObjectProperty ;
                                       owl:propertyChainAxiom ( :actsOnFeatureProperty
                                                                :aboutProperty
                                                              ) .


###  http://www.w3.org/ns/ssn/hasDeployment
<http://www.w3.org/ns/ssn/hasDeployment> rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/ssn/hasProperty
<http://www.w3.org/ns/ssn/hasProperty> rdf:type owl:ObjectProperty ;
                                       owl:inverseOf <http://www.w3.org/ns/ssn/isPropertyOf> ;
                                       rdfs:comment "Relation between an entity and a Property of that entity."@en ,
                                                    "A relation between a FeatureOfInterest and a Property of that feature." ;
                                       rdfs:isDefinedBy <http://www.w3.org/ns/ssn/> ,
                                                        "http://purl.oclc.org/NET/ssnx/ssn" ;
                                       rdfs:label "has property"@en ,
                                                  "has property" ;
                                       rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton" ;
                                       <http://www.w3.org/2004/02/skos/core#definition> "Relation between an entity and a Property of that entity."@en .


###  http://www.w3.org/ns/ssn/hasSubSystem
<http://www.w3.org/ns/ssn/hasSubSystem> rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/ssn/implements
<http://www.w3.org/ns/ssn/implements> rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/ssn/isPropertyOf
<http://www.w3.org/ns/ssn/isPropertyOf> rdf:type owl:ObjectProperty ;
                                        rdfs:comment "Relation between a Property and the entity it belongs to."@en ;
                                        rdfs:isDefinedBy <http://www.w3.org/ns/ssn/> ;
                                        rdfs:label "is property of"@en ;
                                        <http://www.w3.org/2004/02/skos/core#definition> "Relation between a Property and the entity it belongs to."@en .


###  http://www.w3.org/ns/ssn/systems/hasOperatingProperty
<http://www.w3.org/ns/ssn/systems/hasOperatingProperty> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
                                                        rdfs:comment "Relation from an OperatingRange to a Property.  For example, to a battery lifetime." ;
                                                        rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                                                        rdfs:label "has operating property" ;
                                                        rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction" .


###  http://www.w3.org/ns/ssn/systems/hasOperatingRange
<http://www.w3.org/ns/ssn/systems/hasOperatingRange> rdf:type owl:ObjectProperty ;
                                                     rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
                                                     rdfs:comment "Relation from a System to an OperatingRange describing the normal operating environment of the System." ;
                                                     rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                                                     rdfs:label "has operating range" ;
                                                     rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction" .


###  http://www.w3.org/ns/ssn/systems/hasSurvivalProperty
<http://www.w3.org/ns/ssn/systems/hasSurvivalProperty> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
                                                       rdfs:comment "Relation from a SurvivalRange to a Property describing the survial range of a system.  For example, to the temperature extreme that a system can withstand before being considered damaged." ;
                                                       rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                                                       rdfs:label "has survival property" ;
                                                       rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction" .


###  http://www.w3.org/ns/ssn/systems/hasSurvivalRange
<http://www.w3.org/ns/ssn/systems/hasSurvivalRange> rdf:type owl:ObjectProperty ;
                                                    rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
                                                    rdfs:comment "A Relation from a System to a SurvivalRange." ;
                                                    rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                                                    rdfs:label "has survival range" ;
                                                    rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Deploy#OperatingRestriction" .


###  http://www.w3.org/ns/ssn/systems/hasSystemCapability
<http://www.w3.org/ns/ssn/systems/hasSystemCapability> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
                                                       rdfs:comment "Relation from a Sensor to a MeasurementCapability describing the measurement properties of the sensor." ;
                                                       rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                                                       rdfs:label "has measurement  capability" ;
                                                       rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://www.w3.org/ns/ssn/systems/hasSystemProperty
<http://www.w3.org/ns/ssn/systems/hasSystemProperty> rdf:type owl:ObjectProperty ;
                                                     rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
                                                     rdfs:comment "Relation from a MeasurementCapability to a MeasurementProperty.  For example, to an accuracy (see notes at MeasurementCapability)." ;
                                                     rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                                                     rdfs:label "has measurement property" ;
                                                     rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Sensor#MeasuringCapability" .


###  http://www.w3.org/ns/ssn/systems/inCondition
<http://www.w3.org/ns/ssn/systems/inCondition> rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/ssn/systems/qualityOfObservation
<http://www.w3.org/ns/ssn/systems/qualityOfObservation> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
                                                        <http://purl.org/dc/elements/1.1/source> """skos:exactMatch 'resultQuality' [O&M - ISO/DIS 19156] 
		                    http://portal.opengeospatial.org/files/?artifact_id=41579""" ;
                                                        rdfs:comment "Relation linking an Observation to the adjudged quality of the result.  This is of course complimentary to the MeasurementCapability information recorded for the Sensor that made the Observation." ;
                                                        rdfs:isDefinedBy "http://purl.oclc.org/NET/ssnx/ssn" ;
                                                        rdfs:label "quality of observation" ;
                                                        rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Observation#Observation" .


###  http://xmlns.com/foaf/0.1/depiction
<http://xmlns.com/foaf/0.1/depiction> rdf:type owl:ObjectProperty .


###  https://w3id.org/def/saref4bldg#contains
<https://w3id.org/def/saref4bldg#contains> rdf:type owl:ObjectProperty .


###  https://w3id.org/def/saref4bldg#hasSpace
<https://w3id.org/def/saref4bldg#hasSpace> rdf:type owl:ObjectProperty .


###  https://w3id.org/def/saref4bldg#isSpaceOf
<https://w3id.org/def/saref4bldg#isSpaceOf> rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://iot.linkeddata.es/def/core#contractId
:contractId rdf:type owl:DatatypeProperty ;
            rdfs:domain :Contract ;
            rdfs:range xsd:string ;
            rdfs:comment "Identifier of the contract"@en ,
                         "contract identifier"@en .


###  http://iot.linkeddata.es/def/core#dataModelVersion
:dataModelVersion rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Device ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Data model version, equivalent to \"dmv\" field in \"(IoT) DIS 30118-1 - Text for ISO/IEC DIS 30118-1, Information technology — Open Connectivity Foundation — Part 1: Core specification\""@en ;
                  rdfs:label "Data model version"@en .


###  http://iot.linkeddata.es/def/core#description
:description rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:string ;
             rdfs:comment "General attribute for descriptions assigned to any type of concept" ;
             rdfs:label "description"@en .


###  http://iot.linkeddata.es/def/core#deviceDescription
:deviceDescription rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :description ;
                   rdfs:domain :Device ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Natural language description of a device."@en ;
                   rdfs:label "device description"@en .


###  http://iot.linkeddata.es/def/core#deviceName
:deviceName rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf <http://iot.linkeddata.es/def/wot#name> ;
            rdfs:domain :Device ;
            rdfs:range xsd:string ;
            rdfs:comment "Name of a device" ;
            rdfs:label "device name"@en .


###  http://iot.linkeddata.es/def/core#protocolIndependentId
:protocolIndependentId rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Device ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Protocol independent ID, equivalent to \"piid\" field in \"(IoT) DIS 30118-1 - Text for ISO/IEC DIS 30118-1, Information technology — Open Connectivity Foundation — Part 1: Core specification\""@en ;
                       rdfs:label "Protocol independent ID"@en .


###  http://iot.linkeddata.es/def/core#readWrite
:readWrite rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :Contract ;
           rdfs:range xsd:boolean ;
           rdfs:comment "Indicates wether there are read and writes rigths over the service involved in the contrat."@en ,
                        "Read and write rights"@en .


###  http://iot.linkeddata.es/def/core#relativeHref
:relativeHref rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf <http://iot.linkeddata.es/def/wot#href> ;
              rdfs:domain :RelativeEndpoint ;
              rdfs:range xsd:string ;
              rdfs:label "relative href"@en .


###  http://iot.linkeddata.es/def/core#serialNumber
:serialNumber rdf:type owl:DatatypeProperty ;
              rdfs:domain :Device ;
              rdfs:range xsd:string ;
              rdfs:comment "Device serial number" ;
              rdfs:label "serialNumber"@en .


###  http://iot.linkeddata.es/def/core#serviceDescription
:serviceDescription rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :description ;
                    rdfs:domain :Service ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Natural language description of a service."@en ;
                    rdfs:label "service description"@en .


###  http://iot.linkeddata.es/def/core#serviceId
:serviceId rdf:type owl:DatatypeProperty ;
           rdfs:domain :Service ;
           rdfs:range xsd:string ;
           rdfs:comment "Unique dentifier of the service"@en ;
           rdfs:label "service identifier"@en .


###  http://iot.linkeddata.es/def/core#serviceName
:serviceName rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf <http://iot.linkeddata.es/def/wot#name> ;
             rdfs:domain :Service ;
             rdfs:range xsd:string ;
             rdfs:comment "Name of a service" ;
             rdfs:label "service name"@en .


###  http://iot.linkeddata.es/def/core#serviceVersion
:serviceVersion rdf:type owl:DatatypeProperty ;
                rdfs:domain :Service ;
                rdfs:range xsd:string ;
                rdfs:comment "service version"@en .


###  http://iot.linkeddata.es/def/core#softwareVersion
:softwareVersion rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Device ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Software version, equivalent to \"sv\" field in \"(IoT) DIS 30118-1 - Text for ISO/IEC DIS 30118-1, Information technology — Open Connectivity Foundation — Part 1: Core specification\""@en ;
                 rdfs:label "Software version"@en .


###  http://iot.linkeddata.es/def/core#specVersion
:specVersion rdf:type owl:DatatypeProperty ;
             rdfs:domain :Device ;
             rdfs:range xsd:string ;
             rdfs:comment "Specification version, equivalent to \"icv\" field in \"(IoT) DIS 30118-1 - Text for ISO/IEC DIS 30118-1, Information technology — Open Connectivity Foundation — Part 1: Core specification\""@en ;
             rdfs:label "Spec Version"@en ,
                        "Specification version"@en .


###  http://iot.linkeddata.es/def/core#timeStamp
:timeStamp rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:range xsd:dateTime ;
           rdfs:comment "Time stamp in which a phenomenon takes place, for example when value has been taken."@en ;
           rdfs:label "time stamp"@en .


#################################################################
#    Classes
#################################################################

###  http://iot.linkeddata.es/def/core#Actuator
:Actuator rdf:type owl:Class ;
          rdfs:subClassOf :Device ,
                          <http://www.w3.org/ns/sosa/Actuator> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :actsOnFeatureProperty ;
                            owl:allValuesFrom :FeatureProperty
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://www.w3.org/ns/ssn/forProperty> ;
                            owl:allValuesFrom <http://iot.linkeddata.es/def/wot#Property>
                          ] ;
          rdfs:comment "An actuator is a device that accepts digital inputs and which acts on (changes) one or more properties of a physical entity on the basis of those inputs. An actuator is a specialization of an IoT device. An actuator acts on a physical entity. (definition taken from ISO/IEC 11404:2007)"@en ;
          rdfs:label "Actuator"@en ;
          rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf <http://xmlns.com/foaf/0.1/Agent> ;
       rdfs:comment "An agent is a person or piece of software involved in a VICINITY neighbourhood."@en ;
       rdfs:label "Agent"@en .


###  http://iot.linkeddata.es/def/core#Application
:Application rdf:type owl:Class ;
             rdfs:subClassOf :Service ;
             rdfs:comment "An application is a software entity or system that offers a collection of functions with which a user can perform a task or achieve a business objective. An application typically uses services. The functions of an application may be presented as services. (definition taken from ISO/IEC 11404:2007)"@en ;
             rdfs:label "Application"@en ;
             rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#ComposedThing
:ComposedThing rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :isComposedBy ;
                                     owl:someValuesFrom <http://iot.linkeddata.es/def/wot#Thing>
                                   ] ;
               rdfs:comment "This class represent the things (Virtual or Physical) that are composed by other things. While Physicial things can be composed any type of thing, Virtual things can only be composed by Virtual things."@en ;
               rdfs:label "Composed thing"@en .


###  http://iot.linkeddata.es/def/core#Contract
:Contract rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :petitionerItemInvolved ;
                            owl:someValuesFrom :Device
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :petitionerItemInvolved ;
                            owl:someValuesFrom :Service
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :requestedService ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Service
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :serviceOwner ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Agent
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :servicePetitioner ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Agent
                          ] ;
          rdfs:comment "Contract between organizations and requested services."@en ;
          rdfs:label "Contract"@en .


###  http://iot.linkeddata.es/def/core#Device
:Device rdf:type owl:Class ;
        rdfs:subClassOf :PhysicalThing ,
                        <http://www.w3.org/ns/ssn/System> ;
        rdfs:comment "An IoT device is a digital entity which bridges between real-world physical entities and the other digital entities of an IoT system. IoT device interacts one or more networks through which interactions are made with other entities. IoT device exposes one or more endpoints by which interactions are made. (definition taken from ISO/IEC 11404:2007)"@en ;
        rdfs:label "Device"@en ;
        rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#DigitalUser
:DigitalUser rdf:type owl:Class ;
             rdfs:subClassOf <http://xmlns.com/foaf/0.1/Agent> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :interactsUsing ;
                               owl:allValuesFrom :Service
                             ] ;
             rdfs:comment "A digital user is a digital entity which uses an IoT system. A digital user is a specialization of an IoT-User. A digital user interacts with one or more services offered by the IoT system across the network. (definition taken from ISO/IEC 11404:2007)"@en ;
             rdfs:label "Digital user"@en ;
             rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#Ecosystem
:Ecosystem rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :isDescribedBy ;
                             owl:someValuesFrom :ThingEcosystemDescription
                           ] ;
           rdfs:comment "Collection of things that co-exist in a given environment."@en ;
           rdfs:label "Ecosystem"@en .


###  http://iot.linkeddata.es/def/core#FeatureProperty
:FeatureProperty rdf:type owl:Class ;
                 rdfs:comment "Class created following the n-ary class in order to be able to identify which property of feature of interest is afected or monitors by a device or by an interaction pattern. Instances belongin to this class should point to a given (ssn) property and a given feature of interest."@en ;
                 rdfs:label "Feature property"@en .


###  http://iot.linkeddata.es/def/core#HumanUser
:HumanUser rdf:type owl:Class ;
           rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :interactsUsing ;
                             owl:allValuesFrom :Application
                           ] ;
           rdfs:comment "A human user is person who uses an IoT system. A human user is a specialization of an IoT-User. A human user interacts across the network via an application. (definition taken from ISO/IEC 11404:2007)"@en ;
           rdfs:label "Human user"@en ;
           rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#Neighbourhood
:Neighbourhood rdf:type owl:Class ;
               rdfs:comment "A set of IoT infrastructures and value-added services integrated through a network of VICINITY Nodes, where the infrastructures can share the access to their IoT objects without losing the control over them."@en ;
               rdfs:label "Neighbourhood"@en .


###  http://iot.linkeddata.es/def/core#PhysicalThing
:PhysicalThing rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty :isLocatedAt ;
                                 owl:someValuesFrom <http://www.opengis.net/ont/geosparql#Feature>
                               ] ;
               rdfs:comment """A physical entity is a real-world thing. A physical entity is a specialization of entity. A physical entity may contain other physical entities. A physical entity is a discrete, identifiable part of the physical environment that is of interest to the user for the completion of her goal. Physical entities can be almost any physical object or environment; from humans or animals to cars; from store or logistics chain items to computers; from electronic appliances to closed or open environments.

(Definition taken from 
ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA))"""@en ,
                            "Any entity that has a proper space region."@en ;
               rdfs:label "Physical thing"@en ;
               rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#RelativeEndpoint
:RelativeEndpoint rdf:type owl:Class ;
                  rdfs:subClassOf <http://iot.linkeddata.es/def/wot#Link> ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :isRelativeTo ;
                                    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass [ owl:intersectionOf ( <http://iot.linkeddata.es/def/wot#Link>
                                                                       [ rdf:type owl:Class ;
                                                                         owl:complementOf :RelativeEndpoint
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ]
                                  ] ;
                  rdfs:comment "An endpoint which is relative (its href is composed by a relative path and an absolute one given by another endpoint) to an endpoint which can not be relative."@en ;
                  rdfs:label "Relative endpoint"@en .


###  http://iot.linkeddata.es/def/core#Sensor
:Sensor rdf:type owl:Class ;
        rdfs:subClassOf :Device ,
                        <http://www.w3.org/ns/sosa/Sensor> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :monitorsFeatureProperty ;
                          owl:allValuesFrom :FeatureProperty
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://www.w3.org/ns/sosa/observes> ;
                          owl:allValuesFrom <http://iot.linkeddata.es/def/wot#Property>
                        ] ;
        rdfs:comment "A sensor is a device that detects and responds to some type of input from the physical environment and outputs digital data that can be transmitted over a network. A sensor is a specialization of an IoT device. A sensor monitors a physical entity. The output digital data may only be distantly related to the input from the physical environment and may be output with a significant time delay, depending on processing of the data within the device, for example, the recognition of the identity of a person from a surveillance camera device. (definition taken from ISO/IEC 11404:2007)"@en ;
        rdfs:label "Sensor"@en ;
        rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#Service
:Service rdf:type owl:Class ;
         rdfs:subClassOf :VirtualThing ;
         rdfs:comment "A service is a set of distinct capabilities provided by a software component through a defined interface, which may be composed of other services. A service is implemented by one or more components. A service defines network interfaces and exposed by an Endpoint. (definition taken from ISO/IEC 11404:2007)"@en ;
         rdfs:label "Service"@en ;
         rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA)"@en .


###  http://iot.linkeddata.es/def/core#ThingDescription
:ThingDescription rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty :describes ;
                                    owl:someValuesFrom <http://iot.linkeddata.es/def/wot#Thing>
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :describes ;
                                    owl:allValuesFrom <http://iot.linkeddata.es/def/wot#Thing>
                                  ] ;
                  rdfs:comment "A digital representation encapsulates a physical object accessible via Web services"@en ;
                  rdfs:label "Thing description"@en .


###  http://iot.linkeddata.es/def/core#ThingEcosystemDescription
:ThingEcosystemDescription rdf:type owl:Class ;
                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                             owl:onProperty :describes ;
                                             owl:someValuesFrom :Ecosystem
                                           ] ;
                           rdfs:comment "A digital representation that encapsulates an ecosystem taht is accessible via Web services" ;
                           rdfs:label "Thing ecosystem description"@en .


###  http://iot.linkeddata.es/def/core#Value
:Value rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty rdf:value ;
                         owl:someValuesFrom rdfs:Resource
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :expressedInFormat ;
                         owl:allValuesFrom <http://iot.linkeddata.es/def/wot#DataSchema>
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :timeStamp ;
                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:dateTime
                       ] ;
       rdfs:comment "Class representing values provided by an interaction pattern in a given time stamp. It might also indicated the data format in which the value is expressed. It can be also be used to express operating property or measurement capability values for a given device."@en ;
       rdfs:label "Value"@en .


###  http://iot.linkeddata.es/def/core#VirtualThing
:VirtualThing rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :isComposedBy ;
                                owl:allValuesFrom :VirtualThing
                              ] ;
              rdfs:comment """A virtual entity is a digital representation of a physical entity, contained within a service. A virtual entity interacts through an endpoint. A virtual entity represents a physical entity.

(Definition taken from 
ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (IoT RA))"""@en ;
              rdfs:label "Virtual thing"@en ;
              rdfs:seeAlso "ISO/IEC CC 30141. Information technology - Internet of Things Reference architecutre (ioT RA)"@en .


###  http://iot.linkeddata.es/def/wot#Action
<http://iot.linkeddata.es/def/wot#Action> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                            owl:onProperty <http://www.w3.org/ns/ssn/forProperty> ;
                                                            owl:someValuesFrom <http://www.w3.org/ns/ssn/Property>
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :actsOnFeatureProperty ;
                                                            owl:allValuesFrom :FeatureProperty
                                                          ] .


###  http://iot.linkeddata.es/def/wot#Event
<http://iot.linkeddata.es/def/wot#Event> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://www.w3.org/ns/sosa/observes> ;
                                                           owl:someValuesFrom <http://www.w3.org/ns/ssn/Property>
                                                         ] ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :monitorsFeatureProperty ;
                                                           owl:allValuesFrom :FeatureProperty
                                                         ] .


###  http://iot.linkeddata.es/def/wot#Property
<http://iot.linkeddata.es/def/wot#Property> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://www.w3.org/ns/sosa/observes> ;
                                                              owl:someValuesFrom <http://www.w3.org/ns/ssn/Property>
                                                            ] ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :monitorsFeatureProperty ;
                                                              owl:allValuesFrom :FeatureProperty
                                                            ] .


###  http://www.opengis.net/ont/geosparql#Feature
<http://www.opengis.net/ont/geosparql#Feature> rdf:type owl:Class ;
                                               rdfs:subClassOf <http://www.opengis.net/ont/geosparql#SpatialObject> ,
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <http://www.opengis.net/ont/geosparql#hasGeometry> ;
                                                                 owl:allValuesFrom <http://www.opengis.net/ont/geosparql#Geometry>
                                                               ] ;
                                               owl:disjointWith <http://www.opengis.net/ont/geosparql#Geometry> ;
                                               <http://purl.org/dc/elements/1.1/creator> "OGC GeoSPARQL 1.0 Standard Working Group" ;
                                               rdfs:comment "This class represents the top-level feature type. This class is equivalent to GFI_Feature defined in ISO 19156:2011, and it is superclass of all feature types."@en ;
                                               rdfs:isDefinedBy <http://www.opengis.net/ont/geosparql> ;
                                               rdfs:label "Feature"@en .


###  http://www.opengis.net/ont/geosparql#Geometry
<http://www.opengis.net/ont/geosparql#Geometry> rdf:type owl:Class .


###  http://www.opengis.net/ont/geosparql#SpatialObject
<http://www.opengis.net/ont/geosparql#SpatialObject> rdf:type owl:Class ;
                                                     rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                       owl:onProperty <http://www.opengis.net/ont/geosparql#sfContains> ;
                                                                       owl:allValuesFrom <http://www.opengis.net/ont/geosparql#SpatialObject>
                                                                     ] ,
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty <http://www.w3.org/2003/01/geo/wgs84_pos#location> ;
                                                                       owl:allValuesFrom <http://www.w3.org/2003/01/geo/wgs84_pos#Point>
                                                                     ] ;
                                                     <http://purl.org/dc/elements/1.1/creator> "OGC GeoSPARQL 1.0 Standard Working Group" ;
                                                     rdfs:comment "The class spatial-object represents everything that can have a spatial representation. It is superclass of feature and geometry."@en ;
                                                     rdfs:isDefinedBy <http://www.opengis.net/ont/geosparql> ;
                                                     rdfs:label "SpatialObject"@en .


###  http://www.w3.org/2000/01/rdf-schema#Resource
rdfs:Resource rdf:type owl:Class .


###  http://www.w3.org/2003/01/geo/wgs84_pos#Point
<http://www.w3.org/2003/01/geo/wgs84_pos#Point> rdf:type owl:Class .


###  http://www.w3.org/ns/sosa/ActuatableProperty
<http://www.w3.org/ns/sosa/ActuatableProperty> rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> .


###  http://www.w3.org/ns/sosa/Actuator
<http://www.w3.org/ns/sosa/Actuator> rdfs:subClassOf <http://www.w3.org/ns/ssn/System> .


###  http://www.w3.org/ns/sosa/ObservableProperty
<http://www.w3.org/ns/sosa/ObservableProperty> rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> .


###  http://www.w3.org/ns/sosa/Sensor
<http://www.w3.org/ns/sosa/Sensor> rdfs:subClassOf <http://www.w3.org/ns/ssn/System> .


###  http://www.w3.org/ns/ssn/Deployment
<http://www.w3.org/ns/ssn/Deployment> rdf:type owl:Class .


###  http://www.w3.org/ns/ssn/Property
<http://www.w3.org/ns/ssn/Property> rdf:type owl:Class ;
                                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                      owl:onProperty <http://www.w3.org/ns/ssn/isPropertyOf> ;
                                                      owl:someValuesFrom <http://www.w3.org/ns/sosa/FeatureOfInterest>
                                                    ] ,
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty <http://www.w3.org/ns/ssn/isPropertyOf> ;
                                                      owl:allValuesFrom <http://www.w3.org/ns/sosa/FeatureOfInterest>
                                                    ] ;
                                    <http://purl.org/dc/elements/1.1/source> """skos:exactMatch 'property' [O&M] 
		                    http://www.opengeospatial.org/standards/om""" ;
                                    rdfs:comment "A quality of an entity. An aspect of an entity that is intrinsic to and cannot exist without the entity."@en ,
                                                 "A ssn:Property is something that is a DUL:Quality and has a ssn:isPropertyOf property who may be a ssn:FeatureOfInterest"@en ,
                                                 "An observable Quality of an Event or Object.  That is, not a quality of an abstract entity as is also allowed by DUL's Quality, but rather an aspect of an entity that is intrinsic to and cannot exist without the entity and is observable by a sensor." ;
                                    rdfs:isDefinedBy <http://www.w3.org/ns/ssn/> ,
                                                     "http://purl.oclc.org/NET/ssnx/ssn" ,
                                                     "http://purl.oclc.org/NET/ssnx/ssn#" ;
                                    rdfs:label "Property"@en ,
                                               "Property" ,
                                               "property (ssn)" ;
                                    rdfs:seeAlso "http://www.w3.org/2005/Incubator/ssn/wiki/SSN_Skeleton#Skeleton" ;
                                    <http://www.w3.org/2004/02/skos/core#definition> "A quality of an entity. An aspect of an entity that is intrinsic to and cannot exist without the entity."@en .


###  http://www.w3.org/ns/ssn/System
<http://www.w3.org/ns/ssn/System> rdf:type owl:Class ;
                                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                    owl:onProperty <http://www.w3.org/ns/sosa/isHostedBy> ;
                                                    owl:allValuesFrom <http://www.w3.org/ns/sosa/Platform>
                                                  ] ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty <http://www.w3.org/ns/ssn/hasDeployment> ;
                                                    owl:allValuesFrom <http://www.w3.org/ns/ssn/Deployment>
                                                  ] ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty <http://www.w3.org/ns/ssn/hasSubSystem> ;
                                                    owl:allValuesFrom <http://www.w3.org/ns/ssn/System>
                                                  ] ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty <http://www.w3.org/ns/ssn/implements> ;
                                                    owl:allValuesFrom <http://www.w3.org/ns/sosa/Procedure>
                                                  ] ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty [ owl:inverseOf <http://www.w3.org/ns/ssn/hasSubSystem>
                                                                   ] ;
