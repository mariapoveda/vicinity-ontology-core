                                                 rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ,
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty <http://www.w3.org/ns/ssn/systems/hasSurvivalProperty> ;
